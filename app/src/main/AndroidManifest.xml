<?xml version="1.0" encoding="UTF-8"?>

    <manifest
    package="com.example.opsctask2app"
    xmlns:android="http://schemas.android.com/apk/res/android">

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.CAMERA"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>


    <application
        android:theme="@style/Theme.OPSCTASK2APP"
        android:supportsRtl="true"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:label="@string/app_name"
        android:icon="@mipmap/ic_launcher"
        android:allowBackup="true">

    <activity android:name=".wbf_splashscreen">
        <intent-filter>
            <action android:name="android.intent.action.MAIN"/>
            <category android:name="android.intent.category.LAUNCHER"/>
        </intent-filter>
    </activity>

    <activity
        android:name=".wbf_main"
        android:launchMode = "singleTop">
    </activity>

    <activity
        android:name=".wbf_scanner"
        android:parentActivityName=".wbf_dashboard">
    </activity>

    <activity
        android:name=".wbf_user_guide"
        android:parentActivityName=".wbf_splashscreen">
    </activity>

    <activity
        android:name=".wbf_items_graph"
        android:parentActivityName=".wbf_dashboard">
    </activity>

    <activity
        android:name=".wbf_total_items"
        android:parentActivityName=".wbf_dashboard">
    </activity>

    <activity
        android:name=".wbf_total_value"
        android:parentActivityName=".wbf_dashboard">
    </activity>

    <activity
        android:name=".wbf_forgot_password"
        android:parentActivityName=".wbf_login">
    </activity>

    <activity
        android:name=".wbf_add_item"
        android:parentActivityName=".wbf_dashboard">
    </activity>

    <activity
        android:name=".wbf_stock_cat_m"
        android:parentActivityName=".wbf_categories">
    </activity>

    <activity
        android:name=".wbf_item_view_m"
        android:parentActivityName=".wbf_categories">
    </activity>

    <activity
        android:name=".wbf_stock_cat"
        android:parentActivityName=".wbf_browse">
    </activity>

    <activity
        android:name=".wbf_item_view"
        android:parentActivityName=".wbf_browse">
    </activity>

    <activity
        android:name=".wbf_add_cat"
        android:parentActivityName=".wbf_dashboard">
    </activity>

    <activity
        android:name=".wbf_categories"
        android:parentActivityName=".wbf_dashboard">
    </activity>

    <activity
        android:name=".wbf_registration"
        android:parentActivityName=".wbf_main">
    </activity>

    <activity
        android:name=".wbf_login"
        android:parentActivityName=".wbf_main">
    </activity>

    <activity
        android:name=".wbf_browse"
        android:parentActivityName=".wbf_main">
    </activity>

    <activity
        android:name=".wbf_dashboard"
        android:parentActivityName=".wbf_login">
    </activity>

</application>

</manifest>